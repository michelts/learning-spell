    "@types/react-dom": "^18.2.7",
    "drizzle-kit": "^0.20.14",
    "playwright": "^1.43.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.0",
    "wrangler": "^3.48.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.15.6+sha256.01c01eeb990e379b31ef19c03e9d06a14afa5250b82e81303f88721c99ff2e6f"
