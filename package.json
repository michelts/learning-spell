    "@remix-run/react": "^2.8.1",
    "diff": "^5.2.0",
    "drizzle-orm": "^0.30.7",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-audio-voice-recorder": "^2.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.3",
    "ulid-workers": "^2.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@playwright/test": "^1.43.0",
    "@remix-run/dev": "^2.8.1",
    "@types/diff": "^5.0.9",
